#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/bin/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/btoa@1.2.1/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules:/home/home/IdeaProjects/node_modules:/home/home/node_modules:/home/node_modules:/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/extract-loader@5.1.0/node_modules/extract-loader/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/extract-loader@5.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../btoa@1.2.1/node_modules/btoa/bin/btoa.js" "$@"
else
  exec node  "$basedir/../../../../../btoa@1.2.1/node_modules/btoa/bin/btoa.js" "$@"
fi
