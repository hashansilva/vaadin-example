#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/fixtures/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/import-local@2.0.0/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules:/home/home/IdeaProjects/node_modules:/home/home/node_modules:/home/node_modules:/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/webpack-cli@3.3.11_webpack@4.46.0/node_modules/webpack-cli/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/webpack-cli@3.3.11_webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@2.0.0/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../import-local@2.0.0/node_modules/import-local/fixtures/cli.js" "$@"
fi
