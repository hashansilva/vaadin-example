#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/cssesc@3.0.0/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules:/home/home/IdeaProjects/node_modules:/home/home/node_modules:/home/node_modules:/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/css-loader@4.2.1_webpack@4.46.0/node_modules/css-loader/node_modules:/home/home/IdeaProjects/vaadin-example/node_modules/.pnpm/css-loader@4.2.1_webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
fi
